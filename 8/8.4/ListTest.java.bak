import java.util.*;
public class ListTest{
	public static void main(String[] args){
		List books=new ArrayList();
		books.add(new String("123"));
		books.add(new String("456"));
		books.add(new String("789"));

		System.out.println(books);
		books.add(1,new String("321"));
		System.out.println(books);
		books.remove(2);
		System.out.println(books);
		System.out.println(books.indexOf(new String("123")));
		books.set(1,new String("213"));
		System.out.println(books);

		System.out.println(books.subList(2,3));
	}
}